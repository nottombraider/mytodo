 {
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-shadow": "off",
    "newline-after-var": "error",
    "newline-before-return": "error",
    "complexity": ["error", 2],
    "max-lines": ["error", 150],
    "padded-blocks": "error",
    "no-multiple-empty-lines": ["error", { "max": 2 }]
  }
}
